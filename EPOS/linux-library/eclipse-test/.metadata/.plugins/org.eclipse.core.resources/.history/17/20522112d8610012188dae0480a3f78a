#include "Definitions.h"
#include <iostream>
using namespace std;

#define PI 3.14159

void whatever (char* word) {

	cout << word << endl;
}

int main() {

	char DeviceName[] = "EPOS2";
	char ProtocolStackName[] = "CANopen";
	char InterfaceName[] = "USB";
	char PortName[] = "USB0";

	unsigned long ErrorCode;
	void* device;

	whatever(DeviceName);
	device = VCS_OpenDevice(DeviceName.c_str(), ProtocolStackName.c_str(), InterfaceName.c_str(), PortName.c_str(), &ErrorCode);

	return 0;
}


