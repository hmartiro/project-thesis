#include <iostream>
using namespace std;

#define PI 3.14159

int main() {

	int a, b;
	string yo = "yo dawg!!";


	a = 123;

	if (a != 124) {
		cout << a << 'is not 124!';
	}

	cout << yo << endl;

	b = -50;
	cout << a + PI;
	return 0;
}

